{
  "name": "typescript-express-template",
  "version": "1.0.0",
  "description": "",
  "main": "src/boot.ts",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "clean": "rm -rf dist",
    "build": "tsc -p . && mv dist/src/* dist && rm -r dist/src && rm dist/package.json",
    "start": "node -r dotenv/config dist/boot.js",
    "dev": "nodemon --exec 'ts-node' -r dotenv/config src/boot.ts"
  },
  "dependencies": {
    "@cosy-software/node-libraries": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "mongoose": "~5.10.9"
  },
  "devDependencies": {
    "@pulumi/docker": "^2.4.1",
    "@pulumi/gcp": "^4.4.0",
    "@pulumi/pulumi": "^2.14.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.16",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
